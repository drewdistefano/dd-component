    "@babel/preset-react": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.5",
    "jest": "^24.7.1",
    "pm2": "^3.4.1",
    "superagent": "^5.0.2",
    "supertest": "^4.0.2"
